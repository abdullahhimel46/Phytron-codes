/*
    if, n = 4,
 then,a[i]= 2 4 6 8
indices,i = 0 1 2 3 =>(i<n) 
  For printing array using for loop:
  for(int i = 0; i < n; i++){
        printf("%d", &a[i]);
    }
*/
#include <stdio.h>
void fun(int a[], int n,int i){
    //base case
    if(i==n) return; //here, i=4,n=4 => return

    printf("%d ",a[i]);
    fun(a,n,i+1);
    
}
int main()
{
    int n; //suppose, n=4
    scanf("%d", &n);
    int a[n];
    for(int i = 0; i < n; i++){
        scanf("%d", &a[i]);
    }
    fun(a, n,0);
    return 0;
}
