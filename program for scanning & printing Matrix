#include <stdio.h>
int main()
{
    int row,col;
    printf("Enter number of row and column: ");
    scanf("%d%d",&row,&col);
    int a[row][col];

    for(int i = 0; i < row; i++){
        for(int j =0; j< col; j++){
            scanf("%d", &a[i][j]);
         // scanf("%d", &a[i]); => for 1D
        }
    }
    printf("Matrix :\n");
    for(int i = 0; i < row; i++)
    {
        for(int j =0; j<col; j++)
        {
            printf("%d ", a[i][j]);
         // printf("%d", a[i]); => for 1D
        }
        printf("\n");
    }
    return 0;
}
