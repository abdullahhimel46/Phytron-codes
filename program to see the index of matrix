#include <stdio.h>
int main()
{
    int row,col;
    printf("Enter number of row and column: ");
    scanf("%d%d",&row,&col);
    int a[row][col];
   
    printf("Indexes of the matrix :\n");
    for(int i = 0; i < row; i++)
    {
        for(int j =0; j<col; j++)
        {
            printf("a[%d][%d] ", i,j);
        }
        printf("\n");
    }
    return 0;
}
