//counting freqency of integers using stl map
/*
Sample input:
5
1 3 4 4 4
Sample output:
1 : 1
3 : 1
4 : 3
*/
#include <bits/stdc++.h>
using namespace std;

int main()
{
    int n; cin>>n;
    map<int,int> freq;
    for(int i=0; i<n; i++){
        int x; cin>>x;
        freq[x]++;
    }
    
    for(auto k:freq){
        cout<<k.first<<" : "<<k.second<<endl;
    }

    return 0;
}
